node {
   def commit_id
   stage('Preparation') {
     checkout scm
     sh "git rev-parse --short HEAD > .git/commit-id"                        
     commit_id = readFile('.git/commit-id').trim()
   }
   stage('test') {
    
   }
   stage('deploy') {
   shell('mvn package && java -jar target/test-service-jenkins-app.jar')
    
   }
}
